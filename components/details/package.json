{
  "name": "@govuk-react/details",
  "version": "0.9.0",
  "dependencies": {
    "@govuk-react/constants": "^0.9.0",
    "@govuk-react/lib": "^0.9.0",
    "govuk-colours": "^1.1.0",
    "polished": "^4.1.2"
  },
  "peerDependencies": {
    "react": ">=15",
    "styled-components": ">=5.1"
  },
  "scripts": {
    "build": "yarn build:lib && yarn build:es",
    "build:lib": "rimraf lib && babel src -x .js,jsx,.ts,.tsx -d lib --source-maps --config-file ../../babel.config.js",
    "build:es": "rimraf es && cross-env BABEL_ENV=es babel src -x .js,jsx,.ts,.tsx -d es --source-maps --config-file ../../babel.config.js",
    "docs": "doc-component ./lib/index.js ./README.md"
  },
  "main": "src/index.tsx",
  "publishConfig": {
    "main": "lib/index.js",
    "module": "es/index.js",
    "access": "public"
  },

  "author": "Alasdair McLeay",
  "license": "MIT",
  "homepage": "https://github.com/govuk-react/govuk-react/tree/main/components/details",
  "description": "Renders a styled details element along with summary from prop and children rendered as contents.",
  "private": false
}
