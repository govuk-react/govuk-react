{
  "name": "@govuk-react/phase-banner",
  "version": "0.2.12",
  "dependencies": {
    "@govuk-react/constants": "^0.2.7",
    "@govuk-react/hoc": "^0.2.7",
    "@govuk-react/phase-badge": "^0.2.12",
    "govuk-colours": "^1.0.3"
  },
  "peerDependencies": {
    "emotion": ">=9",
    "prop-types": ">=15",
    "react": ">=16.2.0",
