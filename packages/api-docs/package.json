{
  "name": "@govuk-react/api-docs",
  "version": "0.3.1",
  "dependencies": {
    "@babel/node": "7.0.0",
    "@babel/runtime": "7.1.5",
    "chalk": "^2.3.1",
    "commander": "^2.15.0",
    "emotion-server": "^9.1.3",
    "glob": "^7.1.2",
    "glob-promise": "^3.3.0",
    "govuk-react": "^0.3.1",
    "jimp": "^0.2.28",
    "lodash": "^4.17.5",
    "react": "^16.2.0",
    "react-docgen": "^3.0.0-beta11",
    "react-dom": "^16.3.2"
  },
  "devDependencies": {
    "@babel/cli": "7.1.5",
    "@babel/core": "7.1.6",
